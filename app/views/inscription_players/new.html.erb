<h1>New inscription_player</h1>

<% form_for(@inscription_player) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :inscription_id %><br />
    <%= f.text_field :inscription_id %>
  </p>
  <p>
    <%= f.label :player_id %><br />
    <%= f.text_field :player_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', inscription_players_path %>